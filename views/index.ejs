<html>
<head>
	<title>NODE: Foursquare</title>
</head>
<body>
	<form method="GET" action="/">
		<input name="query" placeholder="search"></input>
		<input type="submit"></input>
	</form>
	<div>
		<ul>
		<% for(var i = 0; i < result_list.length; i++) { %>
			<li><%= result_list[i].name %>
				<ul>	
					<li><%= result_list[i].location.address %>
							<%= result_list[i].location.city %>
							<%= result_list[i].location.state %>
							<%= result_list[i].location.postalCode %>
					</li>
					<li><%= result_list[i].contact.phone %></li>
					<li>TOTAL CHECK INS:<%= result_list[i].stats.checkinsCount %></li>
					<li>HERE NOW: <%= result_list[i].checkins.count %></li>
					<li><%= result_list[i].open %></li>
					<li><a href=<%= result_list[i].url %>>Tips</a></li>
				</ul>
			</li> 
		<% } %>
		</ul>
	</div>
</body>
</html>
